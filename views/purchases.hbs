<main class="page catalog-page">
    <section class="clean-block clean-catalog dark">
        <div class="container">
            <div class="block-heading">
                <h2 class="text-info">Purchase Page</h2>
                <p>Welcome employee! This is the list of all the purchases that we have made!</p>
            </div>
            <div class="container">
                <table id="example" class="table table-striped table-bordered" style="width: 100%">
                    <thead>
                    <tr>
                        <th>Customer ID</th>
                        <th>Customer Name</th>
                        <th>Item ID</th>
                        <th>Item Name</th>
                    </tr>
                    </thead>
                    <tbody>
                    {{#each purview as |purview|}}
                        <tr>
                            <td>{{purview.CUSTOMER_ID}}</td>
                            <td>{{purview.CUSTOMER_NAME}}</td>
                            <td>{{purview.Item_ID}}</td>
                            <td>{{purview.Item_Name}}</td>
                        </tr>
                    {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </section>
</main>
